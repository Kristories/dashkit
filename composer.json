{
  "name": "kristories/dashkit",
  "description": "Spatie's laravel-dashboard toollkit",
  "license": "MIT",
  "keywords": [
    "dashkit",
    "dashboard",
    "laravel"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Wahyu Kristianto",
      "email": "w.kristories@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0",
    "spatie/laravel-dashboard": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Kristories\\Dashkit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kristories\\Dashkit\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kristories\\Dashkit\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
